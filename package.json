    "jsdom": "^26.1.0",
    "playwright": "^1.56.1",
    "storybook": "^10.0.2",
    "typescript": "^5.2.2",
    "vite": "^7.0.2",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^3.2.4"
